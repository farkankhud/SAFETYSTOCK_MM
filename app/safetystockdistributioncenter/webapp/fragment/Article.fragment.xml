<core:FragmentDefinition xmlns="sap.ui.comp.valuehelpdialog"
	xmlns:m="sap.m"
	xmlns:fb="sap.ui.comp.filterbar"
	xmlns:core="sap.ui.core">
	<ValueHelpDialog supportRanges="{appModel>/enableArticleSupportRanges}" id="idArticleValueHelp" title="{i18n>article}" ok=".onArticleValueHelpOkPress" cancel=".onArticleValueHelpCancelPress"  key="article" afterClose="onArticleAfterClose">
		<filterBar>
			<fb:FilterBar id="idArticleFilterBar" advancedMode="true" search="onArticleFilterBarSearch">
				<fb:filterGroupItems>	
				</fb:filterGroupItems>
			</fb:FilterBar>
		</filterBar>
	</ValueHelpDialog>
</core:FragmentDefinition>